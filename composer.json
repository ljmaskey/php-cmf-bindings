{
    "name": "ljmaskey/php-cmf-bindings",
    "type": "library",
    "description": "A port of bitcoinclassic/cmf-bindings to PHP",
    "homepage": "https://www.github.com/ljmaskey/php-cmf-bindings",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Lincoln Maskey"
        }
    ],
    "autoload": {
        "psr-4": {
            "ljmaskey\\CmfBindings\\": "src/CmfBindings"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ljmaskey\\Test\\CmfBindings\\": "tests/CmfBindings",
            "ljmaskey\\TestTrait\\CmfBindings\\": "tests/Trait"
        }
    },
    "require": {
        "php-64bit": "^8.2"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3",
        "phpstan/phpstan-strict-rules": "^1.5",
        "phpunit/phpunit": "^10.3",
        "friendsofphp/php-cs-fixer": "^3.26"
    },
    "config": {
        "sort-packages": true
    }
}
